{
	"schemaVersion": 1,
	"id": "undertale-mod",
	"version": "${version}",
	"name": "Undertale Mod",
	"description": "Atatale.",
	"authors": [
		"Ata, Elif"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/undertale-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"atatale.modid.UndertaleMod"
		],
		"client": [
			"atatale.modid.UndertaleModClient"
		],
		"fabric-datagen": [
			"atatale.modid.UndertaleModDataGenerator"
		]
	},
	"mixins": [
		"undertale-mod.mixins.json",
		{
			"config": "undertale-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
